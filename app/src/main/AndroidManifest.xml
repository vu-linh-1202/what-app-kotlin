<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nac.whatappkotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WhatAppKotlin">
        <activity
            android:name=".message.ChatLogAct"
            android:theme="@style/Theme.AppCompat">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".message.LatesMessageAct" />
        </activity>
        <activity
            android:name=".message.LatesMessageAct"
            android:theme="@style/Theme.AppCompat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".registerlogin.LoginAct"
            android:theme="@style/Theme.AppCompat">

        </activity>
        <activity
            android:name=".registerlogin.RegisterActivity"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity
            android:name=".message.NewMessageAct"
            android:theme="@style/Theme.AppCompat">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".registerlogin.RegisterActivity" />
        </activity>
    </application>

</manifest>